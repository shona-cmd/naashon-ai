<?xml version="1.0" encoding="UTF-8"?>
<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="grad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f093fb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f5576c;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="0" dy="2" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background circle -->
  <circle cx="64" cy="64" r="64" fill="url(#grad1)"/>
  
  <!-- Inner circle with glow effect -->
  <circle cx="64" cy="64" r="58" fill="#1a1a2e" filter="url(#shadow)"/>
  
  <!-- Brain-like AI symbol with code elements -->
  <g transform="translate(64, 35)">
    <!-- Left brain lobe -->
    <ellipse cx="-12" cy="0" rx="14" ry="20" fill="url(#grad2)" opacity="0.9"/>
    <!-- Right brain lobe -->
    <ellipse cx="12" cy="0" rx="14" ry="20" fill="url(#grad2)" opacity="0.9"/>
    <!-- Brain connector -->
    <path d="M -8 8 Q 0 15 8 8" stroke="url(#grad2)" stroke-width="2" fill="none" opacity="0.7"/>
  </g>
  
  <!-- Code brackets -->
  <g transform="translate(64, 70)" fill="none" stroke="#667eea" stroke-width="2">
    <!-- Left bracket -->
    <path d="M -20 -5 L -15 0 L -20 5" stroke-linecap="round" stroke-linejoin="round"/>
    <!-- Right bracket -->
    <path d="M 20 -5 L 15 0 L 20 5" stroke-linecap="round" stroke-linejoin="round"/>
  </g>
  
  <!-- Dots representing AI processing -->
  <g fill="url(#grad2)">
    <circle cx="45" cy="80" r="2.5" opacity="0.8"/>
    <circle cx="64" cy="85" r="2.5" opacity="0.8"/>
    <circle cx="83" cy="80" r="2.5" opacity="0.8"/>
  </g>
  
  <!-- Highlight/spark -->
  <circle cx="50" cy="35" r="3" fill="#f093fb" opacity="0.6"/>
  <circle cx="78" cy="40" r="2" fill="#f5576c" opacity="0.5"/>
  
  <!-- Bottom text area indicator -->
  <g fill="none" stroke="#667eea" stroke-width="1.5" opacity="0.6">
    <line x1="40" y1="100" x2="88" y2="100"/>
    <line x1="40" y1="106" x2="85" y2="106"/>
    <line x1="40" y1="112" x2="80" y2="112"/>
  </g>
</svg>
